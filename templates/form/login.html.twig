{% set form = loginForm %}
{% form_theme form 'form/themes/default.html.twig' %}

{{ form_start(form, {
	'attr': {
		'novalidate': true,
		'class': 'base',
	},
}) }}

{{ form_row(form['email']) }}
{{ form_row(form['password']) }}

{#{{ form_row(form['target']) }}#}

{{ form_widget(form['submit'], {
	'attr': {
		'class': 'btn btn-secondary',
	},
}) }}

{{ form_end(form) }}
